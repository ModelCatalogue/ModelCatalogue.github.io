@import 'nib'

a, a:link
	cursor pointer

body
	padding-top 60px

table
	tfoot, tbody
		th
			border-top 1px solid #DDD

footer
	border-top 1px solid #DDD
	margin-top 3em
	height 60px

#keypoints
	background-color: #eeeeee
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	padding: 5px
	height: 100%

.row
	margin-left 0
	margin-right 0